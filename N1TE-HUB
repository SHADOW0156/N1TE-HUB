    local Api = "https://games.roblox.com/v1/games/" .. game.PlaceId .. "/servers/Public?sortOrder=Desc&limit=100"
    local _Servers = HttpService:JSONDecode(game:HttpGet(Api))
    local _Server = _Servers.data[math.random(1, #_Servers.data)]
    TeleportService:TeleportToPlaceInstance(game.PlaceId, _Server.id, LocalPlayer)
end)

local function makeDraggable(frame, handle)
    local dragging, dragInput, dragStart, startPos
    handle.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = frame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then dragging = false end
            end
            )
        end
    end)
    handle.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then dragInput = input end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input == dragInput then
            local delta = input.Position - dragStart
            frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)
end
makeDraggable(MainFrame, TitleBar)
makeDraggable(OpenButton, OpenButton)

OpenButton.MouseButton1Click:Connect(function() MainFrame.Visible = true; OpenButton.Visible = false end)
CloseButton.MouseButton1Click:Connect(function() MainFrame.Visible = false; OpenButton.Visible = true end)

--  

local function getClosestTarget()
    local target = nil
    local dist = Settings.Aimbot.FOV
    
    -- 
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild(Settings.Aimbot.AimPart) then
            local pos, vis = Camera:WorldToViewportPoint(p.Character[Settings.Aimbot.AimPart].Position)
            if vis then
                local mDist = (Vector2.new(pos.X, pos.Y) - UserInputService:GetMouseLocation()).Magnitude
                if mDist < dist then
                    local wallCheck = true
                    if Settings.Aimbot.WallCheck and not Settings.Aimbot.Wallbang then
                        local ray = Ray.new(Camera.CFrame.Position, (p.Character[Settings.Aimbot.AimPart].Position - Camera.CFrame.Position).Unit * 500)
                        local hit = Workspace:FindPartOnRayWithIgnoreList(ray, {LocalPlayer.Character, p.Character})
                        if hit then wallCheck = false end
                    end
                    if wallCheck then target = p.Character dist = mDist end
                end
            end
        end
    end
    
    --
    if Settings.Aimbot.TargetNPCs then
        for _, v in pairs(Workspace:GetDescendants()) do
            if v:IsA("Model") and v:FindFirstChild("Humanoid") and v:FindFirstChild(Settings.Aimbot.AimPart) and not Players:GetPlayerFromCharacter(v) then
                local pos, vis = Camera:WorldToViewportPoint(v[Settings.Aimbot.AimPart].Position)
                if vis then
                    local mDist = (Vector2.new(pos.X, pos.Y) - UserInputService:GetMouseLocation()).Magnitude
                    if mDist < dist then
                        local wallCheck = true
                        if Settings.Aimbot.WallCheck and not Settings.Aimbot.Wallbang then
                            local ray = Ray.new(Camera.CFrame.Position, (v[Settings.Aimbot.AimPart].Position - Camera.CFrame.Position).Unit * 500)
                            local hit = Workspace:FindPartOnRayWithIgnoreList(ray, {LocalPlayer.Character, v})
                            if hit then wallCheck = false end
                        end
                        if wallCheck then target = v dist = mDist end
                    end
                end
            end
        end
    end
    
    return target
end

-- 
local function applyChams(playerChar, isLocalPlayer)
    --
    if isLocalPlayer then
        -- Chams
        if playerChar then
            for _, v in pairs(playerChar:GetDescendants()) do
                if v.Name == "N1TE_Highlight" then
                    v:Destroy()
                end
            end
        end
        return
    end
    
    -- Chams
    if Settings.Visuals.Chams and playerChar then
        for _, v in pairs(playerChar:GetChildren()) do
            if v:IsA("BasePart") then
                local highlight = v:FindFirstChild("N1TE_Highlight") or Instance.new("BoxHandleAdornment")
                highlight.Name = "N1TE_Highlight"
                highlight.Adornee = v
                highlight.AlwaysOnTop = true
                highlight.ZIndex = 10
                highlight.Size = v.Size
                highlight.Transparency = 0.5
                highlight.Color3 = Color3.fromRGB(0, 120, 255)
                highlight.Parent = v
            end
        end
    else
        if playerChar then
            for _, v in pairs(playerChar:GetDescendants()) do
                if v.Name == "N1TE_Highlight" then
                    v:Destroy()
                end
            end
        end
    end
end

-- Chams
for _, p in pairs(Players:GetPlayers()) do 
    if p ~= LocalPlayer then  -- 
        if p.Character then 
            applyChams(p.Character, false) 
        end
        p.CharacterAdded:Connect(function(char)
            applyChams(char, false)
        end)
    end
end

-- 
Players.PlayerAdded:Connect(function(p)
    if p ~= LocalPlayer then  -- 
        p.CharacterAdded:Connect(function(char)
            applyChams(char, false)
        end)
    end
end)


local function isVisible(part)
    if not Settings.Aimbot.WallCheck then return true end
    if Settings.Aimbot.Wallbang then return true end -- إذا كان Wallbang مفعلاً
    local ray = Ray.new(Camera.CFrame.Position, (part.Position - Camera.CFrame.Position).Unit * 1000)
    local hit, pos = Workspace:FindPartOnRayWithIgnoreList(ray, {LocalPlayer.Character, Camera})
    if hit and hit:IsDescendantOf(part.Parent) then return true end
    return false
end

local function updateESP()
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= LocalPlayer then  --ESP
            if p.Character and p.Character:FindFirstChild("Humanoid") and p.Character.Humanoid.Health > 0 then
                local root = p.Character:FindFirstChild("HumanoidRootPart")
                if root then
                    local pos, onScreen = Camera:WorldToViewportPoint(root.Position)
                    if onScreen and Settings.Visuals.Enabled then
                        if not ESPCache[p] then
                            ESPCache[p] = { Box = Drawing.new("Square"), Name = Drawing.new("Text"), Tracer = Drawing.new("Line") }
                        end
                        local esp = ESPCache[p]
                        local color = Color3.fromRGB(0, 120, 255)
                        local headPos = Camera:WorldToViewportPoint(p.Character.Head.Position + Vector3.new(0, 0.5, 0))
                        local legPos = Camera:WorldToViewportPoint(root.Position - Vector3.new(0, 3, 0))
                        local h = math.abs(headPos.Y - legPos.Y)
                        local w = h / 1.5
                        esp.Box.Visible = Settings.Visuals.Boxes
                        esp.Box.Size = Vector2.new(w, h)
                        esp.Box.Position = Vector2.new(pos.X - w/2, pos.Y - h/2)
                        esp.Box.Color = color
                        esp.Box.Thickness = 2
                        esp.Name.Visible = Settings.Visuals.Names
                        esp.Name.Text = p.Name
                        esp.Name.Position = Vector2.new(pos.X, pos.Y - h/2 - 20)
                        esp.Name.Center = true
                        esp.Name.Outline = true
                        esp.Name.Color = color
                        esp.Tracer.Visible = Settings.Visuals.Tracers
                        esp.Tracer.From = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y)
                        esp.Tracer.To = Vector2.new(pos.X, pos.Y + h/2)
                        esp.Tracer.Color = color
                    else
                        if ESPCache[p] then for _, d in pairs(ESPCache[p]) do d.Visible = false end end
                    end
                end
            else
                if ESPCache[p] then for _, d in pairs(ESPCache[p]) do d.Visible = false end end
            end
        end
    end
end

RunService.RenderStepped:Connect(function()
    FOVCircle.Position = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)
    FOVCircle.Radius = Settings.Aimbot.FOV
    FOVCircle.Visible = Settings.Aimbot.Visible and Settings.Aimbot.Enabled

    if Settings.Aimbot.Enabled then
        local target = nil
        local dist = Settings.Aimbot.FOV
        
        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer and p.Character and p.Character:FindFirstChild(Settings.Aimbot.AimPart) then
                if isVisible(p.Character[Settings.Aimbot.AimPart]) then
                    local pos, onScreen = Camera:WorldToViewportPoint(p.Character[Settings.Aimbot.AimPart].Position)
                    if onScreen then
                        local d = (Vector2.new(pos.X, pos.Y) - Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)).Magnitude
                        if d < dist then dist = d; target = p.Character[Settings.Aimbot.AimPart] end
                    end
                end
            end
        end
        
        if Settings.Aimbot.TargetNPCs then
            for _, obj in pairs(Workspace:GetChildren()) do
                if obj:IsA("Model") and obj:FindFirstChild("Humanoid") and obj:FindFirstChild(Settings.Aimbot.AimPart) then
                    local isPlayer = false
                    for _, p in pairs(Players:GetPlayers()) do
                        if p.Character == obj then
                            isPlayer = true
                            break
                        end
                    end
                    if not isPlayer then
                        if isVisible(obj[Settings.Aimbot.AimPart]) then
                            local pos, onScreen = Camera:WorldToViewportPoint(obj[Settings.Aimbot.AimPart].Position)
                            if onScreen then
                                local d = (Vector2.new(pos.X, pos.Y) - Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)).Magnitude
                                if d < dist then dist = d; target = obj[Settings.Aimbot.AimPart] end
                            end
                        end
                    end
                end
            end
        end
        
        if target then 
            Camera.CFrame = Camera.CFrame:Lerp(CFrame.new(Camera.CFrame.Position, target.Position), Settings.Aimbot.Smoothness) 
        end
    end
    
    if Settings.Movement.Fly then
        local root = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if root then
            if not FlyBodyVelocity then
                FlyBodyVelocity = Instance.new("BodyVelocity", root)
                FlyBodyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                FlyBodyGyro = Instance.new("BodyGyro", root)
                FlyBodyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
                LocalPlayer.Character.Humanoid.PlatformStand = true
            end
            FlyBodyVelocity.Velocity = Camera.CFrame.LookVector * Settings.Movement.FlySpeed
            FlyBodyGyro.CFrame = Camera.CFrame
        end
    else
        if FlyBodyVelocity then FlyBodyVelocity:Destroy(); FlyBodyVelocity = nil end
        if FlyBodyGyro then FlyBodyGyro:Destroy(); FlyBodyGyro = nil end
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then LocalPlayer.Character.Humanoid.PlatformStand = false end
    end
    
    if Settings.Movement.NoClip and LocalPlayer.Character then
        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then part.CanCollide = false end
        end
    end
    
   
    if Settings.Visuals.Chams then
        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer and p.Character then      
                for _, v in pairs(p.Character:GetChildren()) do
                    if v:IsA("BasePart") then
                        local highlight = v:FindFirstChild("N1TE_Highlight")
                        if not highlight then
                            highlight = Instance.new("BoxHandleAdornment")
                            highlight.Name = "N1TE_Highlight"
                            highlight.Adornee = v
                            highlight.AlwaysOnTop = true
                            highlight.ZIndex = 10
                            highlight.Size = v.Size
                            highlight.Transparency = 0.5
                            highlight.Color3 = Color3.fromRGB(0, 120, 255)
                            highlight.Parent = v
                        else
                            highlight.Visible = true
                        end
                    end
                end
            end
        end
    else

        for _, p in pairs(Players:GetPlayers()) do
            if p.Character then
                for _, v in pairs(p.Character:GetDescendants()) do
                    if v.Name == "N1TE_Highlight" then
                        v:Destroy()
                    end
                end
            end
        end
    end
    
    pcall(updateESP)
end)

LocalPlayer.Idled:Connect(function()
    if Settings.Utility.AntiAFK then
        game:GetService("VirtualUser"):CaptureController()
        game:GetService("VirtualUser"):ClickButton2(Vector2.new())
    end
end)

UserInputService.JumpRequest:Connect(function()
    if Settings.Movement.InfiniteJump and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid:ChangeState("Jumping")
    end
end)

Tabs[1].Btn.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
Tabs[1].Btn.TextColor3 = Color3.fromRGB(255, 255, 255)
Tabs[1].Container.Visible = true

print("N1TE HUB V6 LOADED SUCCESSFULLY with V10 features!")
